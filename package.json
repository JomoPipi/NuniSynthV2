{
  "name": "nunisynth",
  "productName": "nunisynth",
  "version": "1.0.16",
  "description": "A modular audio graphical software sandbox ecosystem.",
  "main": "src/index.js",
  "scripts": {
    "setup": "electron-installer-windows --src release-builds/NuniSynth-win32-ia32/ --dest installers/",
    "do_it": "electron-packager . NuniSynth --verbose --platform=win32 --arch=x64 --out=release-builds",
    "test": "jest",
    "start": "electron .",
    "dev": "node './scripts/dev.mjs'",
    "package": "electron-forge package",
    "make": "electron-forge make --platform=win32 --arch=x64",
    "publish": "electron-builder --win -p always",
    "publish-mac": "electron-builder --mac -p always",
    "publish-all": "electron-builder --win --mac -p always",
    "lint": "echo \"No linting configured\"",
    "pack": "electron-builder -npm inst-dir",
    "dist": "electron-builder",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . NuniSynth --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"NuniSynth\"",
    "package-linux": "electron-packager . NuniSynth --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "JomoPipi",
        "repo": "NuniSynthv2"
      }
    ],
    "appId": "NuniSynth",
    "productName": "NuniSynth",
    "directories": {
      "output": "build",
      "buildResources": "."
    },
    "mac": {
      "category": "NuniSynth.category.type",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": [
        "NSIS"
      ],
      "icon": "./assets/icon.ico"
    },
    "nsis": {
      "installerIcon": "./assets/icon.ico",
      "uninstallerIcon": "./assets/icon.ico",
      "license": "LICENSE",
      "runAfterFinish": true
    }
  },
  "keywords": [],
  "author": "ronald",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "nunisynth"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.53",
    "@electron-forge/maker-deb": "^6.0.0-beta.53",
    "@electron-forge/maker-rpm": "^6.0.0-beta.53",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.53",
    "@electron-forge/maker-zip": "^6.0.0-beta.53",
    "@types/dom-mediacapture-record": "^1.0.7",
    "electron": "^11.4.2",
    "electron-builder": "^22.10.5",
    "electron-forge": "^5.2.4",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "^15.2.0",
    "electron-prebuilt-compile": "8.2.0",
    "electron-winstaller": "^5.0.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "electron-log": "^4.3.4",
    "electron-updater": "^4.3.8"
  }
}
